{
  "name": "ng-qrcode",
  "description": "Simple AOT compatible QR code generator for your Angular project.",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Michael Nahkies",
    "email": "support@nahkies.co.nz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mnahkies/ng-qrcode.git"
  },
  "bugs": {
    "url": "https://github.com/mnahkies/ng-qrcode/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build-publish-demo-app": "ng build --project=ng-qrcode-demo --prod --base-href \"/ng-qrcode/\" && ngh --dir=./dist/ng-qrcode-demo",
    "test": "ng test",
    "lint": "ng lint",
    "ci-pipeline": "yarn ci-build && yarn ci-test && yarn ci-lint",
    "postci-pipeline": "cp ./README.md ./CHANGES.md ./LICENSE ./dist/ng-qrcode/",
    "ci-build": "ng build --project ng-qrcode --prod && ng build --project=ng-qrcode-demo --prod",
    "ci-test": "ng test --codeCoverage=true",
    "ci-lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.1",
    "@angular/common": "^10.0.1",
    "@angular/compiler": "^10.0.1",
    "@angular/core": "^10.0.1",
    "@angular/forms": "^10.0.1",
    "@angular/platform-browser": "^10.0.1",
    "@angular/platform-browser-dynamic": "^10.0.1",
    "@angular/router": "^10.0.1",
    "core-js": "^3.6.4",
    "qrcode": "^1.4.4",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular-devkit/build-ng-packagr": "~0.1001.4",
    "@angular/cli": "^10.0.0",
    "@angular/compiler-cli": "^10.0.1",
    "@angular/language-service": "^10.0.1",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.0.14",
    "@types/qrcode": "^1.3.4",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  },
  "keywords": [
    "qr",
    "code",
    "qrcode",
    "qr-code",
    "angular",
    "angular2",
    "ng",
    "ngx",
    "ng2"
  ]
}
